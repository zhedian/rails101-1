<p id="notice"><%= notice %></p>

<h1>Mytopics</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mytopics.each do |mytopic| %>
      <tr>
        <td><%= mytopic.title %></td>
        <td><%= mytopic.description %></td>
        <td><%= link_to 'Show', mytopic %></td>
        <td><%= link_to 'Edit', edit_mytopic_path(mytopic) %></td>
        <td><%= link_to 'Destroy', mytopic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mytopic', new_mytopic_path %>
